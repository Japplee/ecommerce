<form action="guardarCliente" method="POST">
    <fieldset>
        <legend>Nuevo Cliente</legend>
        Nombres:
        <input type="text" name="nombres_cliente"><br>
        Apellidos:
        <input type="text" name="apellidos_cliente"><br>
        Telefono:
        <input type="text" name="numero_cliente"><br>
        Direccion:
        <input type="text" name="direccion_cliente"><br>
        Comuna:
        <input type="text" name="comuna_cliente"><br>
        <input type="submit" value="crear">
    </fieldset>
</form>